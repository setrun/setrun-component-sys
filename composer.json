{
    "name": "setrun/setrun-component-sys",
    "description": "System component for SETRUN CMS",
    "keywords": ["yii2", "yii2-extension", "yii2-cms", "cms", "setrun", "setrun-cms", "setrun-extension"],
    "homepage": "http://setrun.ru",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name" : "Alexey Bashkov",
            "email": "ab@smpage.ru",
            "role" : "Ui Ux Designer"
        },
        {
            "name" : "Denis Utkin",
            "email": "dizirator@gmail.com",
            "role" : "Developer",
            "homepage": "http://github.com/dizirator"
        }
    ],
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "~2.0.12",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-queue": "~2.0.0",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-imagine": "^2.1",
        "kartik-v/yii2-icons": "@dev",
        "yii2tech/ar-linkmany": "^1.0",
        "la-haute-societe/yii2-save-relations-behavior": "^1.2",
        "paulzi/yii2-auto-tree" : "^1.0",
        "paulzi/yii2-adjacency-list": "^2.0.3",
        "paulzi/yii2-nested-sets": "^1.0.4",
        "paulzi/yii2-nested-intervals": "^1.0.5",
        "paulzi/yii2-materialized-path": "^2.0.6",
        "codemix/yii2-localeurls": "^1.4",
        "webmozart/assert": "^1.2",
        "needim/noty": "3.1.*",
        "setrun/setrun-component-backend" : "*",
        "setrun/setrun-component-user"    : "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "setrun\\sys\\": "src/"
        }
    },
    "config": {
        "process-timeout": 1800
    }
}
