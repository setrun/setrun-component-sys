{
    "name": "setrun/setrun-sys",
    "description": "Setrun CMS based on php framework Yii2",
    "keywords": ["yii2"," extension"," cms"," setrun", "framework", "SETRUN", "yii2-extension"],
    "homepage": "http://setrun.ru",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name" : "Alexey Bashkov",
            "email": "ab@smpage.ru",
            "role" : "Ui Ux Designer"
        },
        {
            "name" : "Denis Utkin",
            "email": "dizirator@gmail.com",
            "role" : "Developer",
            "homepage": "http://github.com/dizirator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "~2.0.12",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-queue": "~2.0.0",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-imagine": "^2.1",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-icons": "@dev",
        "kartik-v/yii2-date-range": "1.6.7",
        "kartik-v/yii2-field-range": "*",
        "yiidoc/yii2-redactor": "^2.0",
        "yiidoc/yii2-timeago": "^2.0",
        "kotchuprik/yii2-sortable-widgets": "^2.0",
        "2amigos/yii2-date-picker-widget": "^1.0",
        "2amigos/yii2-transliterator-helper": "^0.1.0",
        "matthiasmullie/minify": "^1.3",
        "gkralik/php-uglifyjs": "^1.0",
        "codemix/yii2-localeurls": "^1.4",
        "mongosoft/yii2-upload-behavior": "*",
        "yii2tech/ar-linkmany": "^1.0",
        "la-haute-societe/yii2-save-relations-behavior": "^1.2",
        "paulzi/yii2-auto-tree" : "^1.0",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "name": "yiidoc/yii2-timeago",
            "type": "vcs",
            "url": "git@github.com:dizirator/yii2-timeago.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "setrun\\sys\\": "src/"
        }
    },
    "config": {
        "process-timeout": 1800
    }
}
